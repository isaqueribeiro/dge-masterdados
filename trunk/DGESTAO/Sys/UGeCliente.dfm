    InsertSQL.Strings = (
      'insert into TBCLIENTE'
      
        '  (BAI_COD, BAIRRO, BLOQUEADO, BLOQUEADO_DATA, BLOQUEADO_MOTIVO,' +
        ' BLOQUEADO_USUARIO, '
      
        '   CEP, CID_COD, CIDADE, CNPJ, CODIGO, COMPLEMENTO, DESBLOQUEADO' +
